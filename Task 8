#include "stdafx.h"
#include <iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	double a, b;
	char x;
	cin >> a >> x >> b;
	cout << "=";
	switch (x)
	{
	case('+'):
	{
		cout << a + b << endl;
		break;
	}
	case('-'):
	{
		cout << a - b << endl;
		break;
	}
	case('*'):
	{
		cout << a*b << endl;
		break;
	}
	case('/'):
	{
		cout << a / b << endl;
		break;
	}
	default: {}
			 break;
	}
	return 0;
}
