#include "stdafx.h"
#include <iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	int a, i, s;
	cin >> a;
	s = 0;
	for (i = 1; i <= 11; i++)
	{
		if (a%i == 0)
			s = s + 1;
	}
	if (s >2)
	{
		cout << "Составное число" << endl;
		return 0;
	}
	else
	{
		cout << "Простое число" << endl;
		return 0;
	}
}

//13.
#include "stdafx.h"
#include <iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	int n, i, e, s;
	cin >> n;
	s = 1;
	e = 1;
	for (i = 0; i < n; )
	{
		s = s * 2;
		e = e + 1;
		i = s;
	}
	cout << e << endl;
	return 0;
}
